{
  "name": "osm-gamification-users",
  "version": "0.0.1",
  "description": "User page",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development budo src/index.js:bundle.js --live & npm run watch-sass",
    "build": "NODE_ENV=production browserify src/index.js -o bundle.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint .",
    "build-sass": "node-sass assets/styles/main.scss assets/styles/main.css",
    "watch-sass": "nodemon -e scss -w assets/styles -x 'npm run build-sass'"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react"
          ]
        }
      ],
      [
        "envify"
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/developmentseed/osm-gamification-users.git"
  },
  "author": "Development Seed",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/developmentseed/osm-gamification-users/issues"
  },
  "homepage": "https://github.com/developmentseed/osm-gamification-users",
  "devDependencies": {
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babelify": "^7.2.0",
    "budo": "^7.1.0",
    "envify": "^3.4.0",
    "express": "^4.13.3",
    "node-sass": "3.4.2",
    "clean-css": "3.4.9",
    "nodemon": "1.8.1"
  },
  "dependencies": {
    "babel": "^5.8.34",
    "babel-core": "^5.8.34",
    "babel-eslint": "^4.1.6",
    "browserify": "^12.0.1",
    "cal-heatmap": "^3.5.4",
    "classnames": "^2.2.3",
    "es6-promise": "^3.0.2",
    "eslint": "^1.10.3",
    "eslint-config-standard": "^4.4.0",
    "eslint-plugin-react": "^3.14.0",
    "eslint-plugin-standard": "^1.3.1",
    "history": "^1.17.0",
    "isomorphic-fetch": "^2.2.0",
    "react": "^0.14.5",
    "react-dom": "^0.14.5",
    "react-router": "^1.0.3",
    "react-search-bar": "^1.1.4"
  }
}
